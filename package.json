{
  "name": "skill-report-web",
  "version": "0.1.0",
  "private": true,
  "description": "年間スキル報告書WEB化PJT - AI駆動開発による業務効率化システム",
  "keywords": [
    "nextjs",
    "typescript",
    "prisma",
    "tailwindcss",
    "skill-management"
  ],
  "author": "SAS Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema=src/database/prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate --schema=src/database/prisma/schema.prisma",
    "db:push": "prisma db push --schema=src/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=src/database/prisma/schema.prisma",
    "db:migrate:reset": "prisma migrate reset --schema=src/database/prisma/schema.prisma",
    "db:seed": "tsx scripts/run-seed.ts",
    "db:seed:training": "node scripts/run-training-seed.js",
    "db:studio": "prisma studio --schema=src/database/prisma/schema.prisma",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:neon": "node scripts/test-neon-connection.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:app": "docker-compose logs -f app",
    "docker:logs:db": "docker-compose logs -f postgres",
    "docker:shell:app": "docker-compose exec app sh",
    "docker:shell:db": "docker-compose exec postgres psql -U skill_user -d skill_report_db",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:dev": "docker-compose up --build",
    "docker:dev:detached": "docker-compose up --build -d",
    "docker:db:migrate": "docker-compose exec app npm run db:migrate",
    "docker:db:seed": "docker-compose exec app npm run db:seed",
    "docker:db:seed:training": "docker-compose exec app npm run db:seed:training",
    "docker:db:studio": "docker-compose exec app npm run db:studio"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.14.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@types/xlsx": "^0.0.35",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.379.0",
    "next": "^14.2.29",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.10.1",
    "puppeteer": "^24.16.2",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "recharts": "^2.12.7",
    "redis": "^4.6.13",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.17.57",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.0",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma",
    "seed": "tsx scripts/run-seed.ts"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
